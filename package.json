  "scripts": {
    "build": "nx run-many --all --target=build",
    "commit": "cz",
    "format:check": "nx format:check",
    "lint": "nx run-many --all --target=lint",
    "lint:staged": "lint-staged",
    "start": "nx run-many --all --target=serve",
